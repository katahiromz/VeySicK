100 DIM A(10),B(10)
110 CLS
120 FOR I=1 TO 10
130  A(I)=I*2:B(I)=I*3:GOSUB *PRINTAB
140 NEXT I:LOCATE 0,Y+3
150 FOR I=1 TO 10
160  SWAP A(I),B(I):GOSUB *PRINTAB
170 NEXT I:LOCATE 0,Y+2
180 END
190 *PRINTAB
200 X=POS(0):Y=CSRLIN
210 LOCATE X,Y:PRINT USING "###";A(I);
220 LOCATE X,Y+1:PRINT USING "###";B(I);
230 LOCATE X+5,Y
240 RETURN
